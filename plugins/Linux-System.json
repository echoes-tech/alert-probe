    {
        "id": 1,
        "idAddon": 2, /* File */
        "params": {
            "path": "/proc/meminfo"
        },
        "searches": [
        {
            "id": 1,
            "idType": 1, /* Regex */
            "params": {
                "regex": "^MemTotal:\s+([0-9]+)\skB"
